PROGRAMME max

ENTIER FONCTION max(ENTIER i, ENTIER j)
VAR ENTIER max;
	max=j;
	SI i>j ALORS
		max=i;
	FSI;
	RETOURNE max;
FFONCTION

ENTIER FONCTION min(ENTIER i, ENTIER j)
VAR ENTIER min;
	min=j;
	SI i<j ALORS
		min=i;
	FSI;
	RETOURNE min;
FFONCTION

PRINCIPAL
VAR ENTIER x,y,z;
	z=max(min(1,5),2);
FPRINCIPAL
FPROGRAMME